    </insert>

	<!-- insert -->
	<insert id="batchInsert" parameterType="com.navi.mini.program.model.retboxemp.RetBoxEmp">
        INSERT INTO
        <include refid="Table_Name" />
        <trim prefix="(" suffix=")" suffixOverrides=",">
			ID,
			VALID_FLG,
			BOX_NO,
			PALLET_ID,
			REQ_USER_ID,
			CON_USER_ID,
			PORT_ID,
			EVT_USR,
			EVT_TIMESTAMP,
			RET_BOX_EMP_NO,
        </trim>
		VALUES
		<foreach  collection="list" item="item"  index="index" separator=",">
		(
			#{item.id},
			#{item.boxNo},
			#{item.validFlg},
			#{item.palletId},
			#{item.reqUserId},
			#{item.conUserId},
			#{item.portId},
			#{item.evtUsr},
			#{item.evtTimestamp},
			#{item.retBoxEmpNo},
		)
		</foreach>
