	<!-- insert -->
	<insert id="batchInsert" parameterType="com.navi.mini.program.model.bisuser.BisUser">
        INSERT INTO 
        <include refid="Table_Name" />
        <trim prefix="(" suffix=")" suffixOverrides=",">
			USR_ID,
			UNQ_SEQ_ID,
			USR_KEY,
			USR_NAME,
			DEPT_ID_FK,
			USR_HEAD_FLG,
			VALID_FLG,
			USR_PHS,
			WECHAT,
			TOKEN,
			EVT_USR,
			EVT_TIMESTAMP,
        </trim>
		VALUES
		<foreach  collection="list" item="item"  index="index" separator=",">
		(	
			#{item.usrId},
			#{item.unqSeqId},
			#{item.usrKey},
			#{item.usrName},
			#{item.deptIdFk},
			#{item.usrHeadFlg},
			#{item.validFlg},
			#{item.usrPhs},
			#{item.wechat},
			#{item.token},
			#{item.evtUsr},
			#{item.evtTimestamp},
		)
		</foreach>
    </insert>
	
	<!-- update:修改时不修改createUser、createTime、isDelete -->
    <update id="update" parameterType="com.navi.mini.program.model.bisuser.BisUser">
        UPDATE 
        <include refid="Table_Name" />
        <set>
            <trim suffix=" " suffixOverrides=",">
				<if test="unqSeqId != null">
					UNQ_SEQ_ID = #{unqSeqId},
				</if>
				<if test="usrKey != null">
					USR_KEY = #{usrKey},
				</if>
				<if test="usrName != null">
					USR_NAME = #{usrName},
				</if>
				<if test="deptIdFk != null">
					DEPT_ID_FK = #{deptIdFk},
				</if>
				<if test="usrHeadFlg != null">
					USR_HEAD_FLG = #{usrHeadFlg},
				</if>
				<if test="validFlg != null">
					VALID_FLG = #{validFlg},
				</if>
				<if test="usrPhs != null">
					USR_PHS = #{usrPhs},
				</if>
				<if test="wechat != null">
					WECHAT = #{wechat},
				</if>
				<if test="token != null">
					TOKEN = #{token},
				</if>
				<if test="evtUsr != null">
					EVT_USR = #{evtUsr},
				</if>
				<if test="evtTimestamp != null">
					EVT_TIMESTAMP = #{evtTimestamp},
				</if>
            </trim>
        </set>
		WHERE
